{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "console": {
      "format": "%(asctime)s | %(levelname)-8s | %(name)s | %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S",
      "class": "discord_music_player.utils.logging.ColoredFormatter"
    },
    "file": {
      "format": "%(asctime)s | %(levelname)-8s | %(name)s | %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "console",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "discord_music_player": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
    "discord_music_player.infrastructure": {
      "level": "DEBUG",
      "handlers": ["console"],
      "propagate": false
    },
    "discord_music_player.application": {
      "level": "DEBUG",
      "handlers": ["console"],
      "propagate": false
    },
    "discord_music_player.domain": {
      "level": "DEBUG",
      "handlers": ["console"],
      "propagate": false
    },
    "discord_music_player.infrastructure.discord": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
    "discord_music_player.infrastructure.audio": {
      "level": "DEBUG",
      "handlers": ["console"],
      "propagate": false
    },
    "discord": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
    "discord.client": {
      "level": "WARNING"
    },
    "discord.gateway": {
      "level": "WARNING"
    },
    "discord.http": {
      "level": "WARNING"
    },
    "aiosqlite": {
      "level": "WARNING"
    },
    "urllib3": {
      "level": "WARNING"
    },
    "httpx": {
      "level": "WARNING"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["console"]
  }
}
